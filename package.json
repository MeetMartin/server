{
  "name": "@7urtle/server",
  "version": "1.0.14",
  "description": "Functional server",
  "main": "dist/server.min.js",
  "module": "src/index.js",
  "files": [
    "dist/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "webpack --progress --mode production",
    "update": "npm outdated --depth=0 | grep -v Package | awk '{print $1}' | xargs -I% npm install %@latest --save",
